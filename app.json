{
  "expo": {
    "name": "premise-security-mobile",
    "slug": "premise-security-mobile",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.swinburne.tpsapp",
      "buildNumber": "1.14.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes and take pictures of users' face.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow the app to use your location",
        "NSLocationAlwaysUsageDescription": "Allow the app to use your location",
        "NSLocationWhenInUseUsageDescription": "Allow the app to use your location",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.swinburne.tpsapp",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f20e7caf-c4a9-4422-aabf-b585b9fc2a2a"
      }
    },
    "plugins": [
      "expo-camera",
      "expo-location",
      "expo-font"
    ]
  }
}
